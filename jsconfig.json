{
    "compilerOptions": {
        "target": "es6",
        "module": "node16"
    },
    "exclude": ["node_modules"],
    "editor.codeActionsOnSave": {
        "source.organizeImports": true
    },
    "editor.showUnused": true,
    "editor.autoImportSuggestions.enabled": true,
    "typelens.exclude": ["Object"]
}
